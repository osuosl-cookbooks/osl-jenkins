jenkins:
  authorizationStrategy:
    projectMatrix:
      entries:
      - userOrGroup:
          name: "jenkins"
          permissions:
          - "Agent/Build"
          - "Credentials/Create"
          - "Credentials/Delete"
          - "Credentials/ManageDomains"
          - "Credentials/Update"
          - "Credentials/View"
          - "Job/Build"
          - "Job/Cancel"
          - "Job/Configure"
          - "Job/Create"
          - "Job/Delete"
          - "Job/Discover"
          - "Job/Read"
          - "Job/Workspace"
          - "Overall/Administer"
          - "Overall/Read"
          - "Run/Delete"
          - "Run/Update"
          - "View/Configure"
          - "View/Create"
          - "View/Delete"
          - "View/Read"
      - userOrGroup:
          name: "manatee"
          permissions:
          - "Overall/Administer"
      - userOrGroup:
          name: "osldev"
          permissions:
          - "Agent/Build"
          - "Job/Build"
          - "Job/Cancel"
          - "Job/Configure"
          - "Job/Create"
          - "Job/Delete"
          - "Job/Discover"
          - "Job/Read"
          - "Job/Workspace"
          - "Overall/Read"
          - "Run/Delete"
          - "Run/Update"
          - "View/Configure"
          - "View/Create"
          - "View/Delete"
          - "View/Read"
      - userOrGroup:
          name: "sysadmin"
          permissions:
          - "Agent/Build"
          - "Agent/Configure"
          - "Agent/Connect"
          - "Agent/Create"
          - "Agent/Delete"
          - "Agent/Disconnect"
          - "Job/Build"
          - "Job/Cancel"
          - "Job/Configure"
          - "Job/Create"
          - "Job/Delete"
          - "Job/Discover"
          - "Job/Read"
          - "Job/Workspace"
          - "Overall/Administer"
          - "Overall/Read"
          - "Run/Delete"
          - "Run/Update"
          - "SCM/Tag"
          - "View/Configure"
          - "View/Create"
          - "View/Delete"
          - "View/Read"
  nodeProperties:
  - envVars:
      env:
      - key: "PATH"
        value: "/usr/local/bin:/bin:/usr/bin:/usr/local/sbin:/usr/sbin:/sbin:/var/lib/jenkins/bin:/opt/cinc-workstation/bin:/opt/cinc-workstation/embedded/bin"
  securityRealm:
    ldap:
      configurations:
      - groupSearchBase: "ou=Group"
        inhibitInferRootDN: false
        rootDN: "dc=osuosl,dc=org"
        server: "ldaps://ldap.osuosl.org"
        userSearchBase: "ou=People"
      disableMailAddressResolver: false
      groupIdStrategy: "caseInsensitive"
      userIdStrategy: "caseInsensitive"

