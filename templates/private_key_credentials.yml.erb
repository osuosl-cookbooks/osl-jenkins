credentials:
  system:
    domainCredentials:
    - credentials:
      - basicSSHUserPrivateKey:
          description: "<%= @description %>"
          id: "<%= @name %>"
          privateKeySource:
            directEntry:
              privateKey: ${file:casc_configs/secrets/private_key_credentials_<%= @name %>}
          scope: <%= @scope %>
          username: "<%= @username %>"
