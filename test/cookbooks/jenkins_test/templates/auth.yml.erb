jenkins:
  authorizationStrategy: "loggedInUsersCanDoAnything"
  securityRealm:
    local:
      allowsSignup: false
      enableCaptcha: false
      users:
      - id: "admin"
        name: "admin"
        password: "admin"
        properties:
        - "apiToken"
        - "myView"
        - "timezone"
        - "experimentalFlags"
        - "mailer"
        - preferredProvider:
            providerId: "default"
        - "theme"
